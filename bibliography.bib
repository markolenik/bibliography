@Article{skinner1994,
  author       = {Skinner, Frances K. and Kopell, Nancy and Marder,
                  Eve},
  title        = {Mechanisms for oscillation and frequency control in
                  reciprocally inhibitory model neural networks},
  journal      = {Journal of Computational Neuroscience},
  year         = 1994,
  volume       = 1,
  number       = {1-2},
  pages        = {69--87},
  month        = jun,
  language     = {en},
  issn         = {0929-5313, 1573-6873},
  doi          = {10.1007/BF00962719},
  urldate      = {2015-02-24},
  abstract     = {We describe four different mechanisms that lead to
                  oscillations in a network of two reciprocally
                  inhibitory cells. In two cases (intrinsic release
                  and intrinsic escape) the frequency of the network
                  oscillation is insensitive to the threshold voltage
                  of the synaptic potentials. In the other two cases
                  (synaptic release and synaptic escape) the network
                  frequency is strongly determined by the threshold
                  voltage of the synaptic connections. The distinction
                  between the different mechanisms blurs as the
                  function describing synaptic activation becomes less
                  steep and as the model neurons are removed from the
                  relaxation regime. These mechanisms provide insight
                  into the parameters that control network frequency
                  in motor systems that depend on reciprocal
                  inhibition.},
  file         =
                  {skinner1994-Mechanisms_for_oscillation_and_frequency_control_in_reciprocally_inhibitory.pdf:skinner1994-Mechanisms_for_oscillation_and_frequency_control_in_reciprocally_inhibitory.pdf:PDF},
