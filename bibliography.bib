@InProceedings{kumar2010,
  author          = {Kumar, G. and Aggarwal, V. and Thakor, N.V. and Schieber,
                  M.H. and Kothare, M.V.},
  title           = {Optimal Parameter Estimation of the {{Izhikevich}} Single
                  Neuron Model Using Experimental Inter-Spike Interval ({{ISI}})
                  Data},
  booktitle       = {American {{Control Conference}} ({{ACC}}), 2010},
  year            = 2010,
