}

@Article{park2012,
  author       = {Park, Choongseok and Rubin, Jonathan E.},
  title        = {Cooperation of Intrinsic Bursting and Calcium
                  Oscillations Underlying Activity Patterns of Model
                  Pre-{{B{\"o}tzinger}} Complex Neurons},
  journal      = {Journal of Computational Neuroscience},
  year         = 2012,
  volume       = 34,
  number       = 2,
  pages        = {345--366},
  month        = sep,
  language     = {en},
  issn         = {0929-5313, 1573-6873},
  doi          = {10.1007/s10827-012-0425-5},
  urldate      = {2015-07-11},
  abstract     = {Activity of neurons in the pre-B{\"o}tzinger complex
                  (pre-B{\"o}tC) within the mammalian brainstem drives
                  the inspiratory phase of the respiratory
                  rhythm. Experimental results have suggested that
                  multiple bursting mechanisms based on a
                  calcium-activated nonspecific cationic (CAN)
                  current, a persistent sodium (NaP) current, and
                  calcium dynamics may be incorporated within the
                  pre-B{\"o}tC. Previous modeling works have
                  incorporated representations of some or all of these
                  mechanisms. In this study, we consider a
                  single-compartment model of a pre-B{\"o}tC
                  inspiratory neuron that encompasses particular
                  aspects of all of these features. We present a novel
                  mathematical analysis of the interaction of the
                  corresponding rhythmic mechanisms arising in the
                  model, including square-wave bursting and autonomous
                  calcium oscillations, which requires treatment of a
                  system of differential equations incorporating three
                  slow variables.},
  file         =
                  {park2012-Cooperation_of_intrinsic_bursting_and_calcium_oscillations_underlying_activity.pdf:park2012-Cooperation_of_intrinsic_bursting_and_calcium_oscillations_underlying_activity.pdf:PDF},
  groups       = {Dynamical Systems Theory,Pacemaker Theory},
  keywords     = {Bifurcation analysis,Human Genetics,Multiple
                  bursting
                  mechanisms,Neurology,Neurosciences,Pre-Bötzinger
                  complex,Respiration,Theory of Computation},
  timestamp    = {2015-07-11T19:53:54Z}
}

