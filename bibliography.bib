@Article{soffe1983,
  author       = {Soffe, S. R. and Clarke, J. D. W. and Roberts, Alan},
  title        = {Swimming and Other Centrally Generated Motor
                  Patterns in Newt Embryos},
  journal      = {Journal of comparative physiology},
  year         = 1983,
  volume       = 152,
  number       = 4,
  pages        = {535--544},
  month        = dec,
  language     = {en},
  issn         = {0340-7594, 1432-1351},
  doi          = {10.1007/BF00606443},
  urldate      = {2015-07-20},
  abstract     = {1. Embryos of the newtTriturus vulgaris released
                  from their egg membranes show a variety of centrally
                  programmed behavioural responses that are broadly
                  stimulus specific. 2. Touching the body evokes
                  swimming in which alternating waves of bending pass
                  down the body and propel the animal forwards. In
                  paralysed embryos, this stimulus evokes an
                  appropriate pattern of motor root discharge that
                  alternates on the two sides and shows a
                  rostro-caudal longitudinal delay. This pattern can
                  be seen in spinal animals. 3. Light touch may evoke
                  simple flexures of the body, driven by synchronous
                  bursts of motor root discharge. 4. Strong bilateral
                  pressure to the body elicits a slow lashing
                  movement. In paralysed embryos, this stimulus
                  produces a suitable pattern of large alternating
                  motor root bursts at low frequency. 5. Touching one
                  side of the head evokes a rapid turn away from the
                  side stimulated, usually followed by swimming. In
                  paralysed embryos, the same stimulus leads to a
                  large contralateral motor root burst usually
                  followed by alternating discharge.},
  file         =
                  {soffe1983-Swimming_and_other_centrally_generated_motor_patterns_in_newt_embryos.pdf:soffe1983-Swimming_and_other_centrally_generated_motor_patterns_in_newt_embryos.pdf:PDF},
