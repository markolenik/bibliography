@Article{duan2013,
  author       = {Duan, Lixia and Fan, Denggui and Lu, Qishao},
  title        = {Hopf Bifurcation and Bursting Synchronization in an
                  Excitable Systems with Chemical Delayed Coupling},
  journal      = {Cognitive Neurodynamics},
  year         = 2013,
