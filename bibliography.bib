@Article{jensen2002-1,
  author          = {Jensen, Ole and Tesche, Claudia D.},
  title           = {Frontal Theta Activity in Humans Increases with Memory Load
                  in a Working Memory Task},
  journal         = {European Journal of Neuroscience},
  year            = 2002,
  volume          = 15,
  number          = 8,
  pages           = {1395--1399},
  language        = {en},
  issn            = {1460-9568},
  doi             = {10.1046/j.1460-9568.2002.01975.x},
  urldate         = {2013-08-16},
  abstract        = {Recent theoretical work has suggested that brain
                  oscillations in the theta band are involved in active
                  maintenance and recall of working memory representations. To
                  test this theoretical framework we recorded neuromagnetic
                  responses from 10 subjects performing the Sternberg task.
                  Subjects were required to retain a list of 1, 3, 5 or 7
                  visually presented digits during a 3-s retention period.
                  During the retention period we observed ongoing frontal theta
                  activity in the 7\textendash{}8.5-Hz band recorded by sensors
                  over frontal brain areas. The activity in the theta band
                  increased parametrically with the number of items retained in
                  working memory. A time\textendash{}frequency analysis revealed
                  that the task-dependent theta was present during the retention
                  period and during memory scanning. Following the memory task
                  the theta activity was reduced. These results suggest that
                  theta oscillations generated in frontal brain regions play an
                  active role in memory maintenance.},
  keywords        = {EEG,MEG,prefrontal cortex,short-term memory,Sternberg task},
  timestamp       = {2014-09-30T10:39:32Z}
}

