  groups       = {Synaptic Depression \& Bursting},
  keywords     = {basket cell,hippocampus,Human
                  Genetics,Neurology,Neurosciences,phase-locking,rhythms,short-term
                  plasticity,synchrony,Theory of Computation},
  timestamp    = {2015-02-24T09:47:56Z}
}

@Article{jeffrey2014,
  author       = {Jeffrey, Mike R. and Dankowicz, Harry},
  title        = {Discontinuity-Induced Bifurcation Cascades in Flows
                  and Maps with Application to Models of the Yeast
                  Cell Cycle},
  journal      = {Physica D: Nonlinear Phenomena},
  year         = 2014,
  volume       = 271,
  pages        = {32--47},
  month        = mar,
  issn         = {0167-2789},
  doi          = {10.1016/j.physd.2013.12.011},
  urldate      = {2017-01-26},
  abstract     = {This paper applies methods of numerical continuation
                  analysis to document characteristic bifurcation
                  cascades of limit cycles in piecewise-smooth,
                  hybrid-dynamical-system models of the eukaryotic
                  cell cycle, and associated period-adding cascades in
                  piecewise-defined maps with gaps. A general theory
                  is formulated for the occurrence of such cascades,
                  for example given the existence of a period-two
                  orbit with one point on the system discontinuity and
                  with appropriate constraints on the forward
                  trajectory for nearby initial conditions. In this
                  case, it is found that the bifurcation cascade for
                  nearby parameter values exhibits a scaling
                  relationship governed by the largest-in-magnitude
                  Floquet multiplier, here required to be positive and
                  real, in complete agreement with the characteristic
                  scaling observed in the numerical study. A similar
                  cascade is predicted and observed in the case of a
                  saddle\textendash{}node bifurcation of a period-two
                  orbit, away from the discontinuity, provided that
                  the associated center manifold is found to intersect
                  the discontinuity transversally.},
  file         =
                  {jeffrey2014-Discontinuity-induced_bifurcation_cascades_in_flows_and_maps_with_application.pdf:jeffrey2014-Discontinuity-induced_bifurcation_cascades_in_flows_and_maps_with_application.pdf:PDF},
  groups       = {1D Map},
  keywords     = {Border collisions,Cell division,Grazing
                  bifurcations,Period-adding sequences,Saddle–node
                  bifurcations},
  timestamp    = {2017-01-26T20:50:32Z}
}

@Article{jensen2002,
  author       = {Jensen, Ole and Gelfand, Jack and Kounios, John and
                  Lisman, John E.},
  title        = {Oscillations in the {{Alpha Band}} (9\textendash{}12
                  {{Hz}}) {{Increase}} with {{Memory Load}} during
                  {{Retention}} in a {{Short}}-Term {{Memory Task}}},
  journal      = {Cerebral Cortex},
  year         = 2002,
  volume       = 12,
  number       = 8,
  pages        = {877--882},
  month        = jan,
  language     = {en},
  issn         = {1047-3211, 1460-2199},
  doi          = {10.1093/cercor/12.8.877},
  urldate      = {2013-08-16},
  abstract     = {To study the role of brain oscillations in working
                  memory, we recorded the scalp electroencephalogram
                  (EEG) during the retention interval of a modified
                  Sternberg task. A power spectral analysis of the EEG
                  during the retention interval revealed a clear peak
                  at 9\textendash{}12 Hz, a frequency in the alpha
                  band (8\textendash{}13 Hz). In apparent conflict
                  with previous ideas according to which alpha band
                  oscillations represent brain `idling', we found that
                  the alpha peak systematically increased with the
                  number of items held in working memory. The
                  enhancement was prominent over the posterior and
                  bilateral central regions. The enhancement over
                  posterior regions is most likely explained by the
                  well known alpha rhythm produced close to the
                  parietal-occipital fissure, whereas the lateral
                  enhancement could be explained by sources in
                  somato-motor cortex. A time-frequency analysis
                  revealed that the enhancement was present throughout
                  the last 2.5 s of the 2.8 s retention interval and
                  that alpha power rapidly diminished following the
                  probe. The load dependence and the tight temporal
                  regulation of alpha provide strong evidence that the
                  alpha generating system is directly or indirectly
                  linked to the circuits responsible for working
                  memory. Although a clear peak in the theta band
                  (5\textendash{}8 Hz) was only detectable in one
                  subject, other lines of evidence indicate that theta
                  occurs and also has a role in working
                  memory. Hypotheses concerning the role of alpha band
                  activity in working memory are discussed.},
  file         =
                  {jensen2002-Frontal_theta_activity_in_humans_increases_with_memory_load_in_a_working_memory.pdf:jensen2002-Frontal_theta_activity_in_humans_increases_with_memory_load_in_a_working_memory.pdf:PDF},
  pmid         = 12122036,
  timestamp    = {2014-09-30T10:39:32Z}
}

@Article{jensen2002-1,
  author       = {Jensen, Ole and Tesche, Claudia D.},
  title        = {Frontal Theta Activity in Humans Increases with
                  Memory Load in a Working Memory Task},
  journal      = {European Journal of Neuroscience},
  year         = 2002,
  volume       = 15,
  number       = 8,
  pages        = {1395--1399},
  language     = {en},
  issn         = {1460-9568},
  doi          = {10.1046/j.1460-9568.2002.01975.x},
  urldate      = {2013-08-16},
  abstract     = {Recent theoretical work has suggested that brain
                  oscillations in the theta band are involved in
                  active maintenance and recall of working memory
                  representations. To test this theoretical framework
                  we recorded neuromagnetic responses from 10 subjects
                  performing the Sternberg task. Subjects were
                  required to retain a list of 1, 3, 5 or 7 visually
                  presented digits during a 3-s retention
                  period. During the retention period we observed
                  ongoing frontal theta activity in the
                  7\textendash{}8.5-Hz band recorded by sensors over
                  frontal brain areas. The activity in the theta band
                  increased parametrically with the number of items
                  retained in working memory. A
                  time\textendash{}frequency analysis revealed that
                  the task-dependent theta was present during the
                  retention period and during memory
                  scanning. Following the memory task the theta
                  activity was reduced. These results suggest that
                  theta oscillations generated in frontal brain
                  regions play an active role in memory maintenance.},
  keywords     = {EEG,MEG,prefrontal cortex,short-term
                  memory,Sternberg task},
  timestamp    = {2014-09-30T10:39:32Z}
}

