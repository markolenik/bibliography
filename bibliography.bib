@Article{shilnikov2008,
  author       = {Shilnikov, Andrey and Gordon, Ren{\'e} and Belykh,
                  Igor},
  title        = {Polyrhythmic Synchronization in Bursting Networking
                  Motifs},
  journal      = {Chaos: An Interdisciplinary Journal of Nonlinear
                  Science},
  year         = 2008,
  volume       = 18,
  number       = 3,
  pages        = 037120,
  month        = sep,
  issn         = {1054-1500, 1089-7682},
  doi          = {10.1063/1.2959850},
  urldate      = {2015-04-01},
  abstract     = {We study the emergence of polyrhythmic dynamics of
                  motifs which are the building block for small
                  inhibitory-excitatory networks, such as central
                  pattern generators controlling various locomotive
                  behaviors of animals. We discover that the pacemaker
                  determining the specific rhythm of such a network
                  composed of realistic
                  Hodgkin\textendash{}Huxley-type neurons is
                  identified through the order parameter, which is the
                  ratio of the neurons' burst durations or of duty
                  cycles. We analyze different configurations of the
                  motifs and describe the universal mechanisms for
                  synergetics of the bursting patterns. We discuss
                  also the multistability of inhibitory networks that
                  results in polyrhythmicity of its emergent
                  synchronous behaviors.},
  file         =
                  {shilnikov2008-Polyrhythmic_synchronization_in_bursting_networking_motifs.pdf:shilnikov2008-Polyrhythmic_synchronization_in_bursting_networking_motifs.pdf:PDF;shilnikov2008-Methods_of_the_qualitative_theory_for_the_hindmarsh–rose_model.pdf:shilnikov2008-Methods_of_the_qualitative_theory_for_the_hindmarsh–rose_model.pdf:PDF},
  groups       = {Synaptic Depression \& Bursting},
  keywords     = {Attractors,Bifurcations,Manifolds,Nerve
                  cells,Networks},
  timestamp    = {2015-04-01T14:01:18Z}
}

@Article{shilnikov2008-1,
  author       = {Shilnikov, Andrey and Kolomiets, Marina},
  title        = {Methods of the Qualitative Theory for the
                  Hindmarsh\textendash{}rose Model: A Case Study
                  \textendash{} a Tutorial},
  journal      = {International Journal of Bifurcation and Chaos},
  year         = 2008,
  volume       = 18,
  number       = 08,
  pages        = {2141--2168},
  month        = aug,
  issn         = {0218-1274},
  doi          = {10.1142/S0218127408021634},
  urldate      = {2015-08-20},
  abstract     = {Homoclinic bifurcations of both equilibria and
                  periodic orbits are argued to be critical for
                  understanding the dynamics of the
                  Hindmarsh\textendash{}Rose model in particular, as
                  well as of some square-wave bursting models of
                  neurons of the Hodgkin\textendash{}Huxley type. They
                  explain very well various transitions between the
                  tonic spiking and bursting oscillations in the
                  model. We present the approach that allows for
                  constructing Poincar{\'e} return mapping via the
                  averaging technique. We show that a modified model
                  can exhibit the blue sky bifurcation, as well as, a
                  bistability of the coexisting tonic spiking and
                  bursting activities. A new technique for localizing
                  a slow motion manifold and periodic orbits on it is
                  also presented.},
  groups       = {Dynamical Systems Theory},
  shorttitle   = {Methods of the Qualitative Theory for the
                  Hindmarsh\textendash{}rose Model},
  timestamp    = {2015-08-20T14:11:14Z}
}

@Article{sillar1994,
