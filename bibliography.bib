@Article{shilnikov2005,
  author       = {Shilnikov, Andrey and Cymbalyuk, Gennady},
  title        = {Transition between {{tonic spiking}} and
                  {{bursting}} in a {{neuron model}} via the
                  {{blue}}-{{sky catastrophe}}},
