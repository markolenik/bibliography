@Article{knight1972,
  author          = {Knight, Bruce W.},
  title           = {Dynamics of {{Encoding}} in a {{Population}} of
                  {{Neurons}}},
  journal         = {The Journal of General Physiology},
  year            = 1972,
  volume          = 59,
